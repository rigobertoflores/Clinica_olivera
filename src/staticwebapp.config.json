{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "globalHeaders": {
    "content-security-policy": "style-src-elem 'self'; default-src 'self'; img-src 'self'; script-src 'self'; style-src 'self';"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".webmanifest": "application/manifest+json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}
