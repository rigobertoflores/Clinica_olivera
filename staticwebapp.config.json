{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self'; style-src 'self' 'unsafe-inline';"
      },
    "mimeTypes": {
      ".json": "application/json",
      ".webmanifest": "application/manifest+json"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/404.html"
      }
    }    
  }
  