{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "mimeTypes": {
      ".json": "application/json",
      ".webmanifest": "application/manifest+json"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/404.html"
      }
    },
    "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline';script-src 'self' 'unsafe-inline' 'unsafe-eval';style-src 'self' 'unsafe-inline'; object-src 'none';img-src 'self' data: https://lh3.googleusercontent.com;font-src 'self' data:;"
  }  
  }
  