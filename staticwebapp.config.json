{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self'; style-src 'self' 'unsafe-inline' script-src-attr 'unsafe-inline' font-src 'self' https://fonts.gstatic.com/s/roboto/v30/KFOmCnqEu92Fr1Mu4mxK.woff2;"
       },
    "mimeTypes": {
      ".json": "application/json",
      ".webmanifest": "application/manifest+json"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/404.html"
      }
    }    
  }
  